{
  "name": "texter-web",
  "version": "0.1.0",
  "private": true,
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "firebase": "^4.10.1",
    "firebase-rxjs": "^0.1.2",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "immer": "^9.0.2",
    "papaparse": "^5.3.1",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-file-reader": "^1.1.4",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.5",
    "rebass": "^2.0.0-0",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "redux-saga": "^0.16.0",
    "rxjs": "^5.5.6",
    "serve": "^11.3.2",
    "styled-components": "^5.3.0",
    "styled-system": "^2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "now-start": "serve --auth --single ./build",
    "deploy": "now --public -e SERVE_USER=@serve-user -e SERVE_PASSWORD=@serve-password && now alias \"$(pbpaste)\" texter-web",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "now": {
    "name": "texter-web",
    "alias": "texter-web",
    "public": true,
    "env": {
      "SERVE_USER": "@serve-user",
      "SERVE_PASSWORD": "@serve-password"
    }
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.11.1"
  }
}
